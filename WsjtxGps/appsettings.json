{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Debug",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      },
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "MM/dd/yyy HH:mm:ss ",
        "UseUtcTimestamp": false
      }
    }
  },
  "GPS": {
    "Type": "gpsd"
  },
  
  "GPSSerial": {
    "Poll": 30,
    "Baud": 9600,
    "Port": "/dev/cu.usbserial-AG0JORJQ",
    "ReadTimeout": 1000,
    "WriteTimeout": 1000,
    "DefaultGrid": "EM58ar"
  },
  
  "GPSD": {
    "Poll": 30,
    "Host": "127.0.0.1",
    "Port": 2947,
    "DefaultGrid": "EM58ar"
  },
  
  "Wsjtx": {
    "Listeners": [
      {
        "Ip": "224.0.0.1",
        "Port": 2237,
        "Multicast": true
      },
      {
        "Ip": "127.0.0.1",
        "Port": 2239,
        "Multicast": false
      }
    ]
  }
}
